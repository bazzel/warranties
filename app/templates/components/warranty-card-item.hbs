{{#paper-card as |card|}}
  {{#card.title as |title|}}
    {{#title.text as |text|}}
    {{#text.headline}}{{warranty.name}}{{/text.headline}}
      {{#text.subhead}}Large{{/text.subhead}}
    {{/title.text}}
    {{title.media size="lg" src="https://lorempixel.com/300/300" alt="Tomster" title="Tomster"}}
  {{/card.title}}
  {{#card.actions}}
    {{#paper-button iconButton=true}}{{paper-icon "favorite"}}{{/paper-button}}
    {{#paper-button iconButton=true}}{{paper-icon "settings"}}{{/paper-button}}
    {{#paper-button iconButton=true}}{{paper-icon "share"}}{{/paper-button}}
  {{/card.actions}}
{{/paper-card}}
